{
  "version": 2,
  "name": "travelLogApp",
  "builds": [
    {
      "src": "src/index.js",
      "use": "@now/node-server"
    }
  ],
  "routes": [
    {
      "src": "/.*",
      "dest": "src/index.js"
    }
  ],
  "alias": [
    "travelLog"
  ],
  "env": {
    "NODE_ENV": "production",
    "PORT": "1337",
    "DATABASE_URL": "@travellog2",
    "CORS_ORIGIN": "",
    "PASSWORD": "@travellogpassword"
  }
}